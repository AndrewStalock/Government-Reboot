#include <stdlib.h>
#include <string.h>
#include <stdio.h>
#include "file_disemvowel_test.sh"

int main(int argc, char **argv) {

}

char* disemvowel(FILE* file) { 
  
  fileRead = fopen(file, "r");

  char string;
  scanf("%d", string);
  printf("%d\n", string);
  int i;
  int j = 0;
  int len;
  int count = 0;

  for(i = 0; i < len; ++i){
    if(!(string[i] == 'a' || string[i] == 'e' || string[i] == 'i' || string[i] == 'o' || string[i] == 'u' || string[i] == 'A' || string[i] == 'E' || string[i] == 'I' || string[i] == 'O' || string[i] == 'U')){
      ++count;
    }
  }

  char* noVow;
  noVow = calloc(count + 1, sizeof(char));

  for(i = 0; i < len; ++i){
    if(!(string[i] == 'a' || string[i] == 'e' || string[i] == 'i' || string[i] == 'o' || string[i] == 'u' || string[i] == 'A' || string[i] == 'E' || string[i] == 'I' || string[i] == 'O' || string[i] == 'U')){
      noVow[j] = string[i];
      ++j;
    }
   }

  return noVow;
}
